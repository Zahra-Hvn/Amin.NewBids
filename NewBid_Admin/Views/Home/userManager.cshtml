
@{
    ViewBag.Title = "userManager";
    Layout = "~/Views/Shared/_Layout.cshtml";
}


    <div class="col-xs-12">
        <div class="box">
            <div class="box-header">
                <h3 class="box-title">مدیریت کاربران</h3>
            </div>
            <!-- /.box-header -->
            <div class="box-body">
                <div class="col-md-12 col-sm-12 col-lg-12 col-xs-12">
                    <div style="height:50px;padding-bottom:20px;" class="col-md-3 col-lg-3 col-xs-12 col-sm-6">
                        @using (Html.BeginForm("AllUser","Home",FormMethod.Post))
                        {<input type="submit" style="height:50px;margin-bottom:20px;" class="btn btn-success col-md-12 col-lg-12 col-xs-12 col-sm-12" name="name" value="همه کاربران" />


                        }
                    </div>
                    <div style="padding-bottom:20px;" class="col-md-3 col-lg-3 col-xs-12 col-sm-6">
                        @using (Html.BeginForm("AllComment", "Home", FormMethod.Post))
                        {
                            <input style="height:50px;margin-bottom:20px;" type="submit" class="btn btn-warning col-md-12 col-lg-12 col-xs-12 col-sm-12" name="name" value="پیام های کاربران" />
                        }
                        </div>
                    <div style="height:50px;padding-bottom:20px;" class="col-md-3 col-lg-3 col-xs-12 col-sm-6">
                        @using (Html.BeginForm("Sefareh", "Home", FormMethod.Post))
                        {
                            <input style="height:50px;margin-bottom:20px;" type="submit" class="btn btn-info col-md-12 col-lg-12 col-xs-12 col-sm-12" name="name" value="سفارشات کاربران" />
                        }
                        </div>
                    <div style="height:50px;padding-bottom:20px;" class="col-md-3 col-lg-3 col-xs-12 col-sm-6">
                        <input style="height:50px;margin-bottom:20px;" type="button" class="btn btn-danger col-md-12 col-lg-12 col-xs-12 col-sm-12" name="name" value=" شکایات کاربران" />
                    </div>


                </div>

            </div>
            <!-- /.box-body -->
        </div>
    </div>

<div>
  
</div>



